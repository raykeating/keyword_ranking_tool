{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/fetch_keyword_ranking",  "dest": "/api/fetch_keyword_ranking.php" },
      { "src": "/styles/(.*)", "dest": "/api/styles/$1" },
      { "src": "/images/(.*)", "dest": "/api/images/$1" },
      { "src": "/(.*)",  "dest": "/api/index.php" }
    ]
  }